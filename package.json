{
  "name": "pruebas-jest",
  "version": "1.0.0",
  "description": "Proyecto de pruebas automatizadas con Jest y GitHub Actions",
  "main": "suma.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "keywords": ["jest", "testing", "ci-cd", "github-actions"],
  "author": "",
  "license": "MIT"
}
